{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
 
      "declaration": false,
      "declarationMap": false,

      "noImplicitAny": true,
      "allowSyntheticDefaultImports": true,
    },
    "exclude": ["node_modules/"],
    "allowJs": true,
    "checkJs": true
  }